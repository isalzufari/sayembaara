{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "import/no-extraneous-dependencies": "off",
        "linebreak-style":"off",
        "no-underscore-dangle":"off",  
    }
}
